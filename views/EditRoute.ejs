<head>
    <title>Bus sim</title>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <%- include('../template/layout/header') %>
</head>
<body>
    <form action="/editRoute" method="POST">
        <input hidden value="<%= buses[busIDNo].id %>" name="busID" id="busID">
        <input hidden value="<%=[busIDNo]%>" name="busIDNo" id="busIDNo">
        <input hidden value="<%=routeIDNo %>" name="routeID" id="routeID">
        location: <input value="<%= buses[busIDNo].route[routeIDNo].locName %>" name="locName" id="locName" >
        latitude: <input value="<%= buses[busIDNo].route[routeIDNo].lat %>" name="lat" id="lat">
        longitude: <input value="<%= buses[busIDNo].route[routeIDNo].lng %>" name="lng" id="lng">
        <button id="submitRoute">Submit</button>
    </form>
</body>